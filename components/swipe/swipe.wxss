.swipe {
    width: 100%;
    height: 100rpx;
    font-size: 28rpx;
    line-height: 100rpx;
   
} 
.swipe .content {
    box-sizing: border-box;
    display: inline-block;
    width: 100vw;
    text-align: center;
    /* background-color: gold; */
    height: 100%;
}
.swipe .content .col {
    width: 33.33%;
    overflow: hidden;
    float: left;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.delete {
    background-color: #e4393c;
    color: #fff;
    display: flex;
    
}
.hiddenHandle {
    padding: 0 15px;
    font-size: 32rpx;
    display: inline-block;
    height: 100%;
    vertical-align: top;
    text-align: center;
}
.md {
    overflow: hidden;
    height: 100%;
    box-sizing: border-box;
    background-color: #f8f8f8;
    border-bottom: 1px solid #f0f0f0;
}
.transition {
    transition: all 800ms cubic-bezier(0, 0, 0, 1) 0s;
}

.tags {
    line-height: initial;
    height: 100%;
    display: flex;
    float: none;
    justify-content: center;
    align-items: center;
    flex-wrap: wrap;
}

.tag {
    margin: 4rpx;
    display: inline-block;
    padding: 8rpx 12rpx;
    color: #666;
    background-color: #f0f0f0;
    border-radius: 24rpx;
}